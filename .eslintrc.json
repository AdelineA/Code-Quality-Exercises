 {
    "env": {
        "es2024": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "unicorn"
    ],
   "extends": "plugin:unicorn/recommended",
   "unicorn/filename-case": [
	"error",
	{
		"cases": {
			"camelCase": true
		}
	}
]
}